<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>

	<rule>
		<from>^/simple$</from>
		<to type="permanent-redirect">/extjs4/simple/index.html</to>
	</rule>
	<rule>
		<from>^/direct-named-arguments.html$</from>
		<to type="permanent-redirect">/extjs4/named-arguments.html</to>
	</rule>
	<rule>
		<from>^/direct4.html$</from>
		<to type="permanent-redirect">/extjs4/direct.html</to>
	</rule>
	<rule>
		<from>^/direct-(.*)4\.html$</from>
		<to type="permanent-redirect">/extjs4/$1.html</to>
	</rule>
	

	<rule>
		<from>^/direct.html$</from>
		<to type="permanent-redirect">/extjs3/direct.html</to>
	</rule>
	<rule>
		<from>^/direct-(.*)\.html$</from>
		<to type="permanent-redirect">/extjs3/$1.html</to>
	</rule>	
	
	<rule>
		<from>^/touchexample$</from>
		<to type="permanent-redirect">/touch2/index.html</to>
	</rule>		
	
	
	<rule>
		<from>^/(.*)/simple$</from>
		<to type="permanent-redirect">/$1/extjs4/simple/index.html</to>
	</rule>
	<rule>
		<from>^/(.*)/direct-named-arguments.html$</from>
		<to type="permanent-redirect">/$1/extjs4/named-arguments.html</to>
	</rule>
	<rule>
		<from>^/(.*)/direct4.html$</from>
		<to type="permanent-redirect">/$1/extjs4/direct.html</to>
	</rule>
	<rule>
		<from>^/(.*)/direct-(.*)4\.html$</from>
		<to type="permanent-redirect">/$1/extjs4/$2.html</to>
	</rule>
	

	<rule>
		<from>^/(.*)/direct.html$</from>
		<to type="permanent-redirect">/$1/extjs3/direct.html</to>
	</rule>
	<rule>
		<from>^/(.*)/direct-(.*)\.html$</from>
		<to type="permanent-redirect">/$1/extjs3/$2.html</to>
	</rule>	
	
	<rule>
		<from>^/(.*)/touchexample$</from>
		<to type="permanent-redirect">/$1/touch2/index.html</to>
	</rule>		

</urlrewrite>